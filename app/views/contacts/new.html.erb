<h2>Contacts</h2>
<%= form_for :contacts, url: contacts_path do |f| %>
    <p>
      <%= f.label :email %><br/>
      <%= f.text_field :email %><br/>
    </p>

    <p>
      <%= f.label :message %><br/>
      <%= f.text_area :message %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
