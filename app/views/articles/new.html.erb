<h1>Articles#new</h1>
<%= form_for :articles, url: '/articles' do |f| %>
    <p>
        <%= f.label :title %><br/>
        <%= f.text_field :title %><br/>
    </p>

    <p>
        <%= f.label :text %><br/>
        <%= f.text_area :text %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
